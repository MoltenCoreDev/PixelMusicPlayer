[sub_resource type="GDScript" id=1]
script/source = "extends Control

export(String) var album_title
export(Array, Resource) var tracks

func init(title: String, songs: Array) -> void:
	self.album_title = title
	self.tracks = songs
	for track in tracks:
		var list_entry = load(\"res://Scenes/TrackPanelEntry.tscn\").instance()
		list_entry.track = track
		$SongContainer.add_child(list_entry)
"

